  "main": "index.js",
  "files": [
    "node_modules/ajv",
    "prisma",
    "src",
    "index.js"
  ],
  "scripts": {
    "prisma:generate_my": "prisma generate --schema prisma/prismaMysql/schema.prisma",
    "prisma:format_my": "prisma format --schema prisma/prismaMysql/schema.prisma",
    "prisma:pull_my": "prisma db pull --schema prisma/prismaMysql/schema.prisma",
    "prisma:push_my": "prisma db push --schema prisma/prismaMysql/schema.prisma",
    "prisma:migrate_my": "npx prisma migrate dev --schema prisma/prismaMysql/schema.prisma",
    "prisma:generate_mdb": "prisma generate --schema prisma/prismaMongodb/schema.prisma",
    "prisma:format_mdb": "prisma format --schema prisma/prismaMongodb/schema.prisma",
    "prisma:pull_mdb": "prisma db pull --schema prisma/prismaMongodb/schema.prisma",
    "prisma:push_mdb": "prisma db push --schema prisma/prismaMongodb/schema.prisma",
    "npmlogin": "npm login --scope=@AR3Capital --registry=https://npm.pkg.github.com",
    "publish_npm": "npm publish",
    "test": "node test"
  },
  "devDependencies": {
    "ajv": "6.12.0",
    "ajv-errors": "1.0.1",
    "ajv-keywords": "^3.4.0",
    "prisma": "^4.7.1"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "dotenv": "^16.0.3",
    "express-handlebars": "^6.0.6",
    "nodemailer": "^6.8.0",
    "nodemailer-express-handlebars": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AR3Capital/ar3-manager-utils.git"
  },
  "bugs": {
    "url": "https://github.com/AR3Capital/ar3-manager-utils/issues"
  },
  "homepage": "https://github.com/AR3Capital/ar3-manager-utils#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
